{
  "name": "@francoischalifour/autocomplete.js",
  "description": "Fast and fully-featured autocomplete JavaScript library.",
  "version": "1.0.0-alpha.3",
  "license": "MIT",
  "homepage": "https://github.com/algolia/autocomplete.js",
  "repository": "algolia/autocomplete.js",
  "author": {
    "name": "Algolia, Inc.",
    "url": "https://www.algolia.com"
  },
  "files": [
    "dist/"
  ],
  "source": "src/index.tsx",
  "types": "dist/es/index.d.ts",
  "module": "dist/es/index.js",
  "main": "dist/umd/autocomplete.js",
  "umd:main": "dist/umd/autocomplete.js",
  "unpkg": "dist/umd/autocomplete.js",
  "jsdelivr": "dist/umd/autocomplete.js",
  "sideEffects": false,
  "scripts": {
    "start": "yarn run storybook",
    "test": "jest",
    "test:size": "bundlesize",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "storybook": "start-storybook --quiet --port 6006 --ci --static-dir .storybook/static",
    "storybook:build": "build-storybook --quiet --output-dir website/stories --static-dir .storybook/static",
    "build:umd": "rollup --config",
    "build:es": "babel src --root-mode upward --extensions '.js,.ts,.tsx' --ignore '**/__tests__/**/*','**/__mocks__/**/*','**/__fixtures__/**/*' --out-dir dist/es --quiet",
    "build:prepare": "yarn run build:es",
    "build:types": "tsc --declaration --declarationMap --outFile dist/es/index.d.ts --emitDeclarationOnly",
    "build": "yarn run build:umd && yarn run build:es && yarn run build:types",
    "watch": "babel src --root-mode upward --extensions '.js,.ts,.tsx' --ignore '**/__tests__/**/*','**/__mocks__/**/*','**/__fixtures__/**/*' --out-dir dist/es --quiet --watch",
    "type-check": "tsc --noEmit",
    "website:build": "yarn run build && yarn run storybook:build",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger"
  },
  "dependencies": {
    "downshift": "francoischalifour/downshift#preact-build",
    "preact": "^10.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-typescript": "7.6.0",
    "@storybook/html": "5.2.5",
    "@storybook/theming": "5.2.5",
    "@testing-library/dom": "6.8.1",
    "@testing-library/jest-dom": "4.1.2",
    "@testing-library/preact": "1.0.2",
    "@testing-library/user-event": "7.1.2",
    "@types/algoliasearch": "^3.34.5",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "2.4.0",
    "@typescript-eslint/parser": "2.4.0",
    "algoliasearch": "3.35.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "bundlesize": "0.18.0",
    "eslint": "6.5.1",
    "eslint-config-algolia": "15.0.0",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.19.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "instantsearch.js": "4.0.0",
    "jest": "24.9.0",
    "jest-watch-typeahead": "0.4.0",
    "prettier": "1.18.2",
    "recent-searches": "1.0.1",
    "rollup": "1.25.1",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-license": "0.12.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-terser": "5.1.2",
    "shipjs": "0.8.1",
    "typescript": "3.6.4",
    "webpack": "4.41.2"
  }
}
