{
  "name": "@francoischalifour/autocomplete.js",
  "description": "Fast and fully-featured autocomplete JavaScript library.",
  "version": "1.0.0-alpha.10",
  "license": "MIT",
  "homepage": "https://github.com/algolia/autocomplete.js",
  "repository": "algolia/autocomplete.js",
  "author": {
    "name": "Algolia, Inc.",
    "url": "https://www.algolia.com"
  },
  "files": [
    "dist/"
  ],
  "source": "src/index.tsx",
  "types": "dist/es/index.d.ts",
  "module": "dist/es/index.js",
  "main": "dist/umd/autocomplete.js",
  "umd:main": "dist/umd/autocomplete.js",
  "unpkg": "dist/umd/autocomplete.js",
  "jsdelivr": "dist/umd/autocomplete.js",
  "sideEffects": false,
  "scripts": {
    "start": "yarn run storybook",
    "test": "jest",
    "test:size": "bundlesize",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "storybook": "start-storybook --quiet --port 6006 --ci --static-dir .storybook/static",
    "storybook:build": "build-storybook --quiet --output-dir website/stories --static-dir .storybook/static",
    "build:umd": "rollup --config",
    "build:es": "babel src --root-mode upward --extensions '.js,.ts,.tsx' --ignore '**/__tests__/**/*','**/__mocks__/**/*','**/__fixtures__/**/*' --out-dir dist/es --quiet",
    "build:prepare": "yarn run build:es",
    "build:types": "tsc --declaration --declarationMap --outDir dist/es --emitDeclarationOnly",
    "build": "yarn run build:umd && yarn run build:es && yarn run build:types",
    "watch": "babel src --root-mode upward --extensions '.js,.ts,.tsx' --ignore '**/__tests__/**/*','**/__mocks__/**/*','**/__fixtures__/**/*' --out-dir dist/es --quiet --watch",
    "type-check": "tsc --noEmit",
    "website:build": "yarn run build && yarn run storybook:build",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger"
  },
  "dependencies": {
    "downshift": "https://github.com/francoischalifour/downshift#ac372e473eea78ac7db8775acbae6726adc7e325",
    "preact": "^10.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/plugin-transform-react-jsx": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-typescript": "7.7.7",
    "@storybook/html": "5.2.8",
    "@storybook/theming": "5.2.8",
    "@testing-library/dom": "6.11.0",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/preact": "1.0.2",
    "@testing-library/user-event": "8.0.3",
    "@types/algoliasearch": "^3.34.7",
    "@types/jest": "^24.0.25",
    "@typescript-eslint/eslint-plugin": "2.15.0",
    "@typescript-eslint/parser": "2.15.0",
    "algoliasearch": "3.35.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "bundlesize": "0.18.0",
    "eslint": "6.8.0",
    "eslint-config-algolia": "15.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jest": "23.3.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "instantsearch.js": "4.1.1",
    "jest": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "prettier": "1.19.1",
    "recent-searches": "1.0.5",
    "rollup": "1.29.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-license": "0.13.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-terser": "5.1.3",
    "shipjs": "0.13.1",
    "typescript": "3.7.4",
    "webpack": "4.41.5"
  }
}
