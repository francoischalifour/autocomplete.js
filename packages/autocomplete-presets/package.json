{
  "name": "@francoischalifour/autocomplete-presets",
  "description": "Presets for building autocomplete experiences.",
  "version": "1.0.0-alpha.10",
  "license": "MIT",
  "source": "src/index.ts",
  "types": "dist/esm/index.d.ts",
  "module": "dist/esm/index.js",
  "main": "dist/umd/index.js",
  "umd:main": "dist/umd/index.js",
  "unpkg": "dist/umd/index.js",
  "jsdelivr": "dist/umd/index.js",
  "scripts": {
    "build": "rm -rf ./dist && yarn build:umd && yarn build:esm && yarn build:types",
    "build:esm": "babel src --root-mode upward --extensions '.js,.ts,.tsx' --out-dir dist/esm",
    "build:umd": "rollup --config",
    "build:types": "tsc -p ./tsconfig.declaration.json --outDir ./dist/esm"
  },
  "homepage": "https://github.com/algolia/autocomplete.js",
  "repository": "algolia/autocomplete.js",
  "author": {
    "name": "Algolia, Inc.",
    "url": "https://www.algolia.com"
  },
  "sideEffects": false,
  "files": [
    "dist/"
  ]
}
